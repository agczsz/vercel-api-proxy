{
  "rewrites": [
    { "source": ":/match/:url*", "destination": ":https:/match/:url*" },
    { "source": ":/match/:url*/", "destination": ":https:/match/:url*/" },
    { "source": "/http/:match/:url*", "destination": "http://:match/:url*" },
    { "source": "/http/:match/:url*/", "destination": "http://:match/:url*/" },
    { "source": "/:protocol/:match/:url*", "destination": ":protocol://:match/:url*" },
    { "source": "/:protocol/:match/:url*/", "destination": ":protocol://:match/:url*/" },
    { "source": "/:protocol*/:match/:url*", "destination": ":protocol://:match/:url*" },
    { "source": "/:protocol*/:match/:url*/", "destination": ":protocol://:match/:url*/" }
  ]
}
